
int aa;
int bb;

aa = 1;
bb = 2;

print(100);
print(101);
print(102);
print(103);

printlines(0);
printlines(-10);
printlines(-20);

printlines(1);
print(104, 105, 106, 107);
printlines(2);
print(aa, bb, bb, aa);
printlines(aa);
print(bb, aa, aa, bb);
printlines(bb);
print(aa, aa, bb, bb);
printlines(aa + bb + 2^2);
print(aa, aa, bb, bb);
printlines(bb - aa);

print(1, 2, 3, 4);
printspaces(3); printspaces(-30);
print(5, 6, 7, 8);
printspaces(aa + bb); printspaces(-aa - bb);
print(9, 10, 11, 12);
printspaces(0);
print(13, 14, 15, 16);

int n1;
int n2;
int n3;
int n4;

n1 = 1;
n2 = n1 + 1;
n3 = n1 + n2 + 1;
n4 = n1 + n2 + n3 + 1;

print(n1, n2, n3, n4);
printlines(2);
print(1000);

read(n1, n2, n3, n4);
